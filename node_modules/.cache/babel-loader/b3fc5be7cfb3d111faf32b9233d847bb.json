{"ast":null,"code":"import axios from \"axios\";\nconst Modal = {\n  name: 'Modal',\n  data: () => {\n    return {\n      sender: \"\",\n      message: \"\"\n    };\n  },\n  methods: {\n    toggleOverlay() {\n      this.$emit('showOverlay', false);\n    },\n\n    cancel() {\n      this.sender = \"\";\n      this.message = \"\";\n      this.toggleOverlay();\n    },\n\n    async send() {\n      if (this.sender.length == 0 || this.message.length == 0) return;\n      let validRegex = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/;\n      if (!this.sender.match(validRegex)) return;\n      let data = new FormData();\n      data.append('sender', this.sender);\n      data.append('message', this.message);\n      let result = await axios.post('/script.php', data);\n      this.$emit('displayMessage', result.data.data);\n      this.sender = \"\";\n      this.message = \"\";\n      this.toggleOverlay();\n    }\n\n  }\n};\nexport default Modal;","map":{"version":3,"mappings":"AAsFA,OAAOA,KAAP,MAAkB,OAAlB;AAEA,MAAMC,KAAI,GAAI;EACVC,IAAI,EAAE,OADI;EAEVC,IAAI,EAAE,MAAM;IACR,OAAO;MACHC,MAAM,EAAE,EADL;MAEHC,OAAO,EAAE;IAFN,CAAP;EAIH,CAPS;EAQVC,OAAO,EAAE;IACLC,aAAa,GAAG;MACZ,KAAKC,KAAL,CAAW,aAAX,EAA0B,KAA1B;IACH,CAHI;;IAILC,MAAM,GAAG;MACL,KAAKL,MAAL,GAAc,EAAd;MACA,KAAKC,OAAL,GAAe,EAAf;MAEA,KAAKE,aAAL;IACH,CATI;;IAUL,MAAMG,IAAN,GAAa;MAET,IAAG,KAAKN,MAAL,CAAYO,MAAZ,IAAsB,CAAtB,IAA2B,KAAKN,OAAL,CAAaM,MAAb,IAAuB,CAArD,EACI;MAEJ,IAAIC,UAAS,GAAI,sEAAjB;MAEA,IAAG,CAAC,KAAKR,MAAL,CAAYS,KAAZ,CAAkBD,UAAlB,CAAJ,EACI;MAEJ,IAAIT,IAAG,GAAI,IAAIW,QAAJ,EAAX;MAEAX,IAAI,CAACY,MAAL,CAAY,QAAZ,EAAsB,KAAKX,MAA3B;MACAD,IAAI,CAACY,MAAL,CAAY,SAAZ,EAAuB,KAAKV,OAA5B;MAEA,IAAIW,MAAK,GAAI,MAAMhB,KAAK,CAACiB,IAAN,CAAW,aAAX,EAA0Bd,IAA1B,CAAnB;MAEA,KAAKK,KAAL,CAAW,gBAAX,EAA6BQ,MAAM,CAACb,IAAP,CAAYA,IAAzC;MAEA,KAAKC,MAAL,GAAc,EAAd;MACA,KAAKC,OAAL,GAAe,EAAf;MAEA,KAAKE,aAAL;IACJ;;EAjCK;AARC,CAAd;AA4CA,eAAeN,KAAf","names":["axios","Modal","name","data","sender","message","methods","toggleOverlay","$emit","cancel","send","length","validRegex","match","FormData","append","result","post"],"sourceRoot":"","sources":["/Users/codyjohns/Documents/empire-american/src/components/Modal.vue"],"sourcesContent":["<template>\n    <div class=\"overlay\">\n        <div class=\"modal-cont\">\n            <div style=\"position:relative\">\n                <button class=\"btn btn-outline-danger\" style=\"position: absolute;right:0px;\" @click=\"toggleOverlay\">X</button>\n            </div>\n            <div class=\"col-12\" style=\"padding-top:20px;text-align: left !important;\">\n                <h3>Leave a message:</h3>\n            </div>\n            <form @submit.prevent=\"send\">\n                <div class=\"col-12\" style=\"padding-top:20px;text-align: left !important;\">\n                    <input type=\"email\" v-model=\"sender\" placeholder=\"Email...\" required/>\n                </div>\n                <div class=\"col-12\" style=\"padding-top:20px;text-align: left !important;\">\n                    <textarea v-model=\"message\" placeholder=\"Type your message here...\" required></textarea>\n                </div>\n                <div class=\"col-12 overlay-bottom\">\n                    <div style=\"padding-top:20px;text-align: left !important;\">\n                        <button class=\"btn btn-outline-light\">Send</button>\n                    </div>\n                </div>\n            </form>\n        </div>\n    </div>\n</template>\n\n<style>\n.overlay {\n    width: 100%;\n    height: 100vh;\n    background-color: rgba(0, 0, 0, 0.6);\n    position: fixed;\n    z-index: 1001;\n    display: flex;\n    justify-content: center;\n    align-items: center;\n}\n\n.overlay-bottom {\n    width:100%;\n    height:70px;\n    background: #d90429;\n}\n\n.modal-cont {\n    width:90vw;\n    max-width: 800px;\n    background-color: #fff;\n    border-radius: 6px;\n}\n\ninput {\n    border: none;\n    outline: none;\n    border-bottom: 2px solid #d90429;\n    border-left: 2px solid #d90429;\n    border-top: 2px solid #e5e5e5;\n    border-right: 2px solid #e5e5e5;\n    width: 70%;\n    padding: 8px;\n}\n\ninput:focus {\n    border-top: 2px solid #ef233c;\n    border-right: 2px solid #ef233c;\n}\n\ntextarea {\n    border: none;\n    outline: none;\n    border-bottom: 2px solid #d90429;\n    border-left: 2px solid #d90429;\n    border-top: 2px solid #e5e5e5;\n    border-right: 2px solid #e5e5e5;\n    width: 100%;\n    height: 300px;\n    padding: 8px;\n}\n\ntextarea:focus {\n    border-top: 2px solid #ef233c;\n    border-right: 2px solid #ef233c;\n}\n</style>\n\n<script>\nimport axios from \"axios\"\n\nconst Modal = {\n    name: 'Modal',\n    data: () => {\n        return {\n            sender: \"\",\n            message: \"\"\n        }\n    },\n    methods: {\n        toggleOverlay() {\n            this.$emit('showOverlay', false);\n        },\n        cancel() {\n            this.sender = \"\";\n            this.message = \"\";\n\n            this.toggleOverlay();\n        },\n        async send() {\n\n            if(this.sender.length == 0 || this.message.length == 0)\n                return;\n\n            let validRegex = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\\.[a-zA-Z0-9-]+)*$/;\n\n            if(!this.sender.match(validRegex))\n                return;\n\n            let data = new FormData();\n\n            data.append('sender', this.sender);\n            data.append('message', this.message);\n\n            let result = await axios.post('/script.php', data);\n\n            this.$emit('displayMessage', result.data.data);\n\n            this.sender = \"\";\n            this.message = \"\";\n\n            this.toggleOverlay();\n        }\n    }\n}\nexport default Modal\n</script>"]},"metadata":{},"sourceType":"module"}